}
.main .section__slider .pagination-area span {
  display: inline-block;
  width: 10px;
  height: 10px;
  border-radius: 50%;
  background-color: rgba(0, 0, 0, 0.8);
  margin-right: 5px;
  transform: scale(0.5);
  transition: 0.3s ease-in-out;
  opacity: 0.4;
}
.main .section__slider .pagination-area span.active {
  transform: scale(1);
  opacity: 1;
